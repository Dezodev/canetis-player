{
    "trailingComma": "es5",
    "tabWidth": 4,
    "printWidth": 100,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "semi": false,
    "singleQuote": true,
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": ["^@/", "^@assets/", "^@components/", "^@utils/", "^[./]"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
